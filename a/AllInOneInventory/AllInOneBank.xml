	<Scripts>
      <OnEvent>
        AIOBFrame_OnEvent(event);
        AIOBTitle_Update();
      </OnEvent>
      <OnLoad>
        AIOBFrame_OnLoad();
        --this:RegisterForClicks("LeftButtonUp", "RightButtonUp");
        AIOBTitle_Update();
      </OnLoad>
      <OnHide>
        AIOBFrame_OnHide();
      </OnHide>
      <OnShow>
        AIOBFrame_OnShow();
      </OnShow>
      <OnMouseDown>
        if ( arg1 == "LeftButton" ) then
          if ( AIOBFreeze == 0 ) then
            this:StartMoving();
          else
            AIOB_Print(AIOB_FROZEN_ERROR);
          end
        end
      </OnMouseDown>
      <OnMouseUp>
        if ( arg1 == "LeftButton" ) then
          this:StopMovingOrSizing();
        end
      </OnMouseUp>
  </Scripts>
