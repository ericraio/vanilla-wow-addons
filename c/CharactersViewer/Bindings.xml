<Bindings>
   <Binding name="CHARACTERSVIEWER_TOGGLE" header="CHARACTERSVIEWER">
      CharactersViewer.Toggle();
   </Binding>   
	<Binding name="CHARACTERSVIEWER_BANKTOGGLE" header="CHARACTERSVIEWER_BANK_TITLE">
      CharactersViewer.Bank.Toggle();
   </Binding>
   <Binding name="CHARACTERSVIEWER_SWITCH_PREVIOUS">
      CharactersViewer.Switch(-1);
   </Binding>
   <Binding name="CHARACTERSVIEWER_SWITCH_NEXT">
      CharactersViewer.Switch(1);
   </Binding>
</Bindings>
