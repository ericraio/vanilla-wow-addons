				this:SetBackdropColor(TOOLTIP_DEFAULT_BACKGROUND_COLOR.r, TOOLTIP_DEFAULT_BACKGROUND_COLOR.g, TOOLTIP_DEFAULT_BACKGROUND_COLOR.b);
				this:SetOwner(WorldFrame, "ANCHOR_NONE");
			</OnLoad>
		</Scripts>
