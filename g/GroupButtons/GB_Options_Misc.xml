	<Layers>
		<Layer level="ARTWORK">
			<FontString name="$parent_Label" inherits="GameFontNormal" justifyH="LEFT" text="GB_COLORS">
				<Color r="1.0" g="1.0" b="0"/>
				<Anchors>
					<Anchor point="LEFT" relativePoint="TOPRIGHT">
