					GB_Set_Anchor("_DisableOutOfRangeSpam", 100, 0);
				</OnLoad>
			</Scripts>
		</Frame>
		<Slider name="$parent_ClickboxX1" inherits="GB_SliderTemplate" minValue="-50" maxValue="50"  valueStep="1" id="555">
