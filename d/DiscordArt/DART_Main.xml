<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/"> 

<Script file="localization.lua"/>
<Script file="DART_Textures.lua"/>
<Script file="DART_Default.lua"/>
<Script file="DART_Custom.lua"/>
<Script file="DART_Functions.lua"/>
<Script file="DART_Main.lua"/>

<Frame name="DART_MainFrame" hidden="true" parent="UIParent">
	<Scripts>
		<OnLoad>
			DART_Main_OnLoad();
		</OnLoad>
	</Scripts>
</Frame>

<Button name="DART_TextureTemplate" inherits="DiscordLib_TooltipBackdropTemplate" parent="UIParent" frameStrata="BACKGROUND" movable="true" virtual="true">
	<Size><AbsDimension x="20" y="20"/></Size>
	<Layers>
		<Layer level="ARTWORK">
			<Texture name="$parent_Texture">
				<Color r="1" g="1" b="1"/>
				<Anchors>
					<Anchor point="TOPLEFT"/>
					<Anchor point="BOTTOMRIGHT"/>
				</Anchors>
			</Texture>
		</Layer>
		<Layer level="OVERLAY">
			<FontString name="$parent_Text" inherits="GameFontNormal"/>
			<Texture name="$parent_Highlight" alphaMode="ADD" file="Interface\AddOns\DiscordLibrary\PlainBackdrop" hidden="true">
				<Anchors>
					<Anchor point="TOPLEFT"/>
					<Anchor point="BOTTOMRIGHT"/>
				</Anchors>
			</Texture>
		</Layer>
	</Layers>
	<Scripts>
		<OnLoad>
			DART_Texture_OnLoad();
		</OnLoad>
		<OnUpdate>
			DART_Texture_OnUpdate();
		</OnUpdate>
		<OnEvent>
			DART_Texture_OnEvent();
		</OnEvent>
		<OnEnter>
			DART_Texture_OnEnter();
		</OnEnter>
		<OnLeave>
			DART_Texture_OnLeave();
		</OnLeave>
		<OnShow>
			DART_Texture_OnShow();
		</OnShow>
		<OnHide>
			DART_Texture_OnHide();
		</OnHide>
		<OnClick>
			DART_Texture_OnClick();
		</OnClick>
		<OnMouseUp>
			DART_Texture_OnMouseUp();
		</OnMouseUp>
		<OnMouseDown>
			DART_Texture_OnMouseDown();
		</OnMouseDown>
		<OnMouseWheel>
			DART_Texture_OnMouseWheel();
		</OnMouseWheel>
		<OnReceiveDrag>
			DART_Texture_OnReceiveDrag();
		</OnReceiveDrag>
		<OnDragStart>
			DART_Texture_OnDragStart();
		</OnDragStart>
		<OnDragStop>
			DART_Texture_OnDragStop();
		</OnDragStop>
	</Scripts>
</Button>

<Button name="DART_Texture_1" inherits="DART_TextureTemplate" id="1"/>

</Ui>