			<OnLeave>
				GameTooltip:Hide();
			</OnLeave>
			<OnClick>
				BankItems_Bag_OnClick(this:GetID());
			</OnClick>
			<OnLoad>
				this.isBag = 1;
			</OnLoad>
		</Scripts>
