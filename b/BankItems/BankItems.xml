			<Frame name="$parentHighlightFrame" setAllPoints="true">
				<Layers>
					<Layer>
						<Texture name="$parentTexture" alphaMode="ADD" file="Interface\Buttons\CheckButtonHilight" setAllPoints="true" hidden="true"/>									
