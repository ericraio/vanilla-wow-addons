<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\FrameXML\UI.xsd">
	<Script file="localization.lua"/>
	<Script file="TitanSkills.lua"/>
	<Frame parent="UIParent">
		<Frames>
			<Button name="TitanPanelSkillsButton" inherits="TitanPanelComboTemplate" frameStrata="FULLSCREEN" toplevel="true">
				<Scripts>
					<OnLoad>
						TitanPanelSkillsButton_OnLoad();
						TitanPanelButton_OnLoad();
					</OnLoad>
					<OnEvent>
						TitanPanelSkillsButton_OnEvent();
					</OnEvent>
					<OnClick>
						TitanPanelButton_OnClick(arg1);
						TitanPanelSkillsButton_OnClick(arg1);
					</OnClick>
				</Scripts>
			</Button>
			<Button name="TitanPanelSkillsRightButton" inherits="TitanPanelIconTemplate" frameStrata="FULLSCREEN" toplevel="true">
				<Scripts>
					<OnLoad>
						TitanPanelSkillsRightButton_OnLoad();
						TitanPanelButton_OnLoad();
					</OnLoad>

					<OnClick>
						TitanPanelButton_OnClick(arg1);
						TitanPanelSkillsButton_OnClick(arg1);
					</OnClick>

				</Scripts>
			</Button>
		</Frames>
	</Frame>	
</Ui>