<Bindings>
    <Binding name="MAILTOLOG" description="Display MailTo log" header="MAILTO">
        MailTo_command('');
    </Binding>
    <Binding name="MAILTOEX" description="Display Inbox expirations">
        MailTo_expire('active');
    </Binding>
    <Binding name="MAILTOMAIL" description="Toggle MailTo Inbox window">
        MailTo_inbox('');
    </Binding>
</Bindings>
