		<HighlightFont name="$parentHighlightText" inherits="GameFontHighlightSmall" justifyH="LEFT"/>
		<DisabledFont	name="$parentDisabledText" inherits="GameFontDisableSmall" justifyH="LEFT"/>
	</Button>

