	<Button name="MonkeyQuestButtonTemplate" virtual="true">
		<Layers>
			<Layer level="ARTWORK">
				<FontString name="$parentText" inherits="MonkeyQuestFont" justifyH="LEFT">
					<Anchors>
						<Anchor point="TOPLEFT">
						</Anchor>
					</Anchors>
				</FontString>
			</Layer>
			<Layer level="BACKGROUND">
				<Texture name="$parentTexture" file="Interface\AddOns\MonkeyLibrary\Textures\ButtonBackDrop.tga" />
			</Layer>
		</Layers>
		<Scripts>
			<OnLoad>
				MonkeyQuestButton_OnLoad();
			</OnLoad>
			<OnClick>
				MonkeyQuestButton_OnClick(arg1);
			</OnClick>
			<OnEnter>
				MonkeyQuest_OnEnter();
				MonkeyQuestButton_OnEnter();
			</OnEnter>
			<OnLeave>
				MonkeyQuest_OnLeave();
				GameTooltip:Hide();
			</OnLeave>
		</Scripts>
	</Button>
