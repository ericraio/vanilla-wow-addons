			<OnLoad>
				Wardrobe.ButtonOnLoad();
			</OnLoad>
			<OnEnter>
				Wardrobe.HandleUIMenuTriggerEnter();
			</OnEnter>
			<OnLeave>
				Wardrobe.HandleUIMenuTriggerLeave();
			</OnLeave>
			<OnClick>
				Wardrobe.HandleUIMenuTriggerClick();
			</OnClick>
			<OnDragStart>
				Wardrobe.OnDragStart(this);
			</OnDragStart>
			<OnDragStop>
				Wardrobe.OnDragStop(this);
			</OnDragStop>
	   </Scripts>
